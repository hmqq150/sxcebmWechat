/* pages/Customer/DealProfit/DealProfit.wxss */
@import '../../common.wxss';

.page {
  background-color: #f5f5f5;
}

.page {
  min-height: 100vh;
}

.weui-msg__text-area {
  width: 94%;
  box-sizing: border-box;
  margin: 0 auto 64rpx auto;
  padding: 32rpx !important;
  background-color: #fff;
  border-radius: 10rpx;
}
.weui-msg{
  background-color: transparent !important;
}
.weui-form-preview__label{
  color: #000 !important;
  font-size: 30rpx;
}
.weui-form-preview::before,.weui-form-preview::after,.weui-cells::after,.weui-cells::before{
  border:none !important;
}
.code_lines{
  font-size: 30rpx;
  color: #999;
  line-height: 1.5;
}
.van-hairline--top-bottom::after,
.van-cell::after {
    border: none !important;
}
.input_box {
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  border: 1px solid #ddd;
}
.bottom_btn {
  width: 94%;
  margin: 20rpx auto;
}
